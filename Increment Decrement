package com.edu.operator;

public class MainApp1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b;
		a=10;
		b=10;
		//c=++a + b++;
	System.out.println(a+b+"sum");
	System.out.println("sum" +a+b);
	
	System.out.println((a+b)+"sum");
	System.out.println("sum" +(a+b));

	}

}
